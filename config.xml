 <?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"

        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.bsdmasterindex.hackerDojo.cal"
        version     = "1.1"
        versionCode = "10" >

        <!-- versionCode is optional and Android only -->

        <preference name="phonegap-version" value="3.5.0" />

        <name>hackerDojo.cal</name>

        <description>
        Hacker Dojo (unoffical) Calender App
        </description>

        <author href="http://bsdmasterindex.com" email="jessemonroy650@yahoo.com">
        Jesse Monroy
        </author>
        <gap:platform name="android" />

        <icon src="drawable-ldpi/icon.png" gap:platform="android" gap:qualifier="ldpi" />
        <icon src="drawable-mdpi/icon.png" gap:platform="android" gap:qualifier="mdpi" />
        <icon src="drawable-hdpi/icon.png" gap:platform="android" gap:qualifier="hdpi" />
        <icon src="drawable-xhdpi/icon.png" gap:platform="android" gap:qualifier="xhdpi" />

        <preference name="permissions" value="none"/>

        <gap:plugin name="org.apache.cordova.splashscreen" version="0.3.3" />
    </widget>
